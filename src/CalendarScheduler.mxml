<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   xmlns:ns1="*"
			   xmlns:components="com.etherpros.components.*"
			   minWidth="955" minHeight="600" 
			   initialize="init()"
			   applicationComplete="applicationComplete()">

	
	
	<fx:Script>
		<![CDATA[
			import com.etherpros.components.RigView;
			
			import mx.controls.Alert;
			import mx.core.UIComponent;
			import mx.skins.ProgrammaticSkin;
			
			// Static reference to main application.
			public static var INSTANCE:Application;
			
			private function init():void {
				// set static instance variable to the instantiated main app.
				INSTANCE = this;
			}
			
			private function applicationComplete():void {
				var ui:RigView = new RigView(400,200);
				ui.width = 100;
				tst.addElement(ui);
			}
			
			protected function button1_clickHandler(event:MouseEvent):void
			{
				Alert.show("Click me");
				
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>

	<s:Group id="tst" y="550" x="50">		
		<components:RigView id="rigview" width="100" height="15" />						   
	</s:Group>
		

	<components:CalendarForm x="27" y="38">
	</components:CalendarForm>
	
</s:Application>
